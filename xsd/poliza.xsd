	<xsd:include schemaLocation="./tipoPoliza.xsd" />
	
	<xsd:element name="RequestPoliza">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="id" type="xsd:int"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	
	<xsd:element name="ResponsePoliza">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="poliza" type="ns1:Poliza" />
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	
</xsd:schema>
